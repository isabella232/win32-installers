<?xml version="1.0"?>
<project name="Gtk# Installer" default="build">
  <target name="*">
    <nant target="${target::get-current-target()}">
      <buildfiles> 
        <include name="modules/*.build" />
        <include name="runtime-en/*.build" />
        <include name="sdk-en/*.build" />
      </buildfiles>
    </nant>
  </target>
</project>
